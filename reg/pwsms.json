{
  "sensor_url": "https://bluehorizon.network/microservices/pwsms",
  "sensor_name": "pwsms",
  "sensor_version": "1.0.0",
  "auto_upgrade": true,
  "active_upgrade": true,
  "attributes": [
    {
      "id": "compute",
      "short_type": "compute",
      "label": "Personal Weather Station",
      "publishable": true,
      "mappings": {
        "ram": 128,
        "cpus": 1
      }
    },
    {
      "id": "app",
      "short_type": "mapped",
      "label": "app",
      "publishable": true,
      "mappings": {
        "HZN_PWS_MODEL": "$PWS_MODEL",
        "MTN_PWS_MODEL": "$PWS_MODEL",
        "HZN_PWS_ST_TYPE": "$PWS_ST_TYPE",
        "MTN_PWS_ST_TYPE": "$PWS_ST_TYPE"
      }
    }
  ]
}
