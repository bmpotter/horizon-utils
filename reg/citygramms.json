{
  "sensor_url": "https://bluehorizon.network/microservices/citygramms",
  "sensor_name": "citygramms",
  "sensor_version": "1.0.0",
  "auto_upgrade": true,
  "active_upgrade": true,
  "attributes": [
    {
      "id": "compute",
      "short_type": "compute",
      "label": "NYU Citygram (noise pollution analysis)",
      "publishable": true,
      "mappings": {
        "ram": 256,
        "cpus": 1
      }
    },
    {
      "id": "app",
      "short_type": "mapped",
      "label": "app",
      "publishable": false,
      "mappings": {
        "HZN_CG_EMAIL": "$CITYGRAM_CG_EMAIL",
        "HZN_CG_PASS": "$CITYGRAM_CG_PASS",
        "HZN_CG_RSDNAME": "$CITYGRAM_CG_RSDNAME"
      }
    }
  ]
}
