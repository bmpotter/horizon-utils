{
  "sensor_url": "https://bluehorizon.network/documentation/netspeed-device-api",
  "sensor_name": "netspeed",
  "attributes": [
    {
      "id": "app",
      "short_type": "mapped",
      "label": "app",
      "publishable": true,
      "mappings": {
        "MTN_IS_BANDWIDTH_TEST_ENABLED": "true",
        "HZN_IS_BANDWIDTH_TEST_ENABLED": "true",
        "MTN_TARGET_SERVER": "$NETSPEED_TARGET_SERVER",
        "HZN_TARGET_SERVER": "$NETSPEED_TARGET_SERVER"
      }
    },
    {
      "id": "compute",
      "short_type": "compute",
      "label": "Compute Resources",
      "publishable": true,
      "mappings": {
        "ram": 128,
        "cpus": 1
      }
    }
  ]
}
