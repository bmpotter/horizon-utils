{
  "sensor_url": "https://bluehorizon.network/documentation/pws-device-api",
  "sensor_name": "pws",
  "attributes": [
    {
      "id": "app",
      "short_type": "mapped",
      "label": "app",
      "publishable": true,
      "mappings": {
        "HZN_WUGNAME": "$PWS_WUGNAME",
        "HZN_PWS_MODEL": "$PWS_MODEL",
        "MTN_PWS_MODEL": "$PWS_MODEL",
        "HZN_PWS_ST_TYPE": "$PWS_ST_TYPE",
        "MTN_PWS_ST_TYPE": "$PWS_ST_TYPE"
      }
    },
    {
      "id": "compute",
      "short_type": "compute",
      "label": "Personal Weather Station",
      "publishable": true,
      "mappings": {
        "ram": 128,
        "cpus": 1
      }
    }
  ]
}
